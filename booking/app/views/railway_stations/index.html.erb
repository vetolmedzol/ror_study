<p id="notice"><%= notice %></p>

<h1>Railway Stations</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @railway_stations.each do |railway_station| %>
    <tr>
      <td><%= railway_station.title %></td>
      <td><button><%= link_to 'Show', railway_station %></button></td>
      <td><button><%= link_to 'Edit', edit_railway_station_path(railway_station) %></button></td>
      <td><button><%= link_to 'Destroy', railway_station, method: :delete, data: { confirm: 'Are you sure?' } %></button></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<button> <%= link_to 'New Railway Station', new_railway_station_path %> </button>
<button> <%= link_to 'Back','welcome#index' %> </button>
