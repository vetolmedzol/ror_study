<p id="notice"><%= notice %></p>


<%= form_with(model: [:admin, railway_station], local: true) do |form| %>
  <% render 'common/errors', resource: railway_station %>

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div class="actions">
    <%= form.submit t('.submit', action: @railway_station.new_record? ? t('common.create') : t('common.edit')) %>
  </div>
<% end %>
